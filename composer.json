{
  "name": "fakerphp/core",
  "type": "library",
  "description": "Faker Core contains the reference implementations for extensions",
  "keywords": [
    "faker",
    "core",
    "interface"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "bram",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Faker\\Core\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
